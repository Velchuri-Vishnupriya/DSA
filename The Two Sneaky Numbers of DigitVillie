//T.C : O(n)
//S.C : O(n)
class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        vector<int> vec;
        unordered_map<int,int> mp;
        for(int i = 0; i < nums.size(); i++){
mp[nums[i]]++;
        }
        for(auto& it : mp){
            if(it.second == 2){
                vec.push_back(it.first);
                if(vec.size() == 2) break;
            }
        }
    return vec;}
};
